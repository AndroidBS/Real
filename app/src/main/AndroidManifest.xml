<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.idp100.real">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_robot"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Main2Activity" />
        <activity android:name=".Control" />
        <activity android:name=".Manual" />
        <activity android:name=".Music" />
        <activity android:name=".Auto" />
        <activity android:name=".Bluetooth" />
        <activity android:name=".Voice" />
        <activity android:name=".Voi_Top" />
        <activity android:name=".MusicPlayer" />
        <activity android:name=".Talkback" />
        <activity android:name=".DeviceList" />
        <activity android:name=".ledControl" />

        <service
            android:name=".MyService"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name=".BluetoothServices"
            android:enabled="true"
            android:exported="true" />

        <service
            android:name=".StartService"
            android:enabled="true"
            android:exported="true" />
    </application>

</manifest>